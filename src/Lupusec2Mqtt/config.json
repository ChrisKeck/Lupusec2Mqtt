{
  "name": "Lupusec2Mqtt",
  "version": "1.1.0",
  "slug": "Lupusec2Mqtt",
  "description": "Connect your Lupusec Alarm system to HomeAssistant by an MQTT integration",
  "startup": "application",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "options": {
    "log_level": "info",
    "seconds_between_quotes": 5
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "seconds_between_quotes": "int(1,120)"
  },
  "image": "cyberdns/lupusec2mqtt"
}
